include_rules

# Linker Symbol Inputs
&UNKSYM = not-found-sym.ld

: ssb64.in.ld |> $(CPP) -P -o %o %f |> ssb64.ld {cpp-ld}

: {cpp-ld} | <audio_objs> <asm_objs> <resources> <imgbanks> <unknowns> <unres_syms>\
|> $(LD) $(LD_FLAGS) -T %f &(UNKSYM) %<unres_syms> -o $(ELF) \
   %<audio_objs> %<asm_objs> %<resources> %<imgbanks> %<unknowns> \
|> $(ELF) | $(TARGET).map

: $(ELF) |> !cpy_rom |> $(ROM)